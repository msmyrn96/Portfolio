        "@testing-library/user-event": "^12.1.10",
        "ahooks": "^3.8.1",
        "axios": "^1.7.5",
        "babel-jest": "^29.7.0",
        "bootstrap": "^5.2.0-beta1",
        "emailjs-com": "^3.2.0",
